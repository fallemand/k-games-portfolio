{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "no-underscore-dangle": [2, {"allow": ["__PRELOADED_STATE__"]}],
    "no-unused-vars": [1, {"argsIgnorePattern": "res|next|^err|reject", "ignoreRestSiblings": true}],
    "react/forbid-prop-types": [0],
    "jsx-a11y/no-static-element-interactions": [0],
    "jsx-a11y/click-events-have-key-events": [0],
    "object-curly-newline": [0],
    "camelcase": [1],
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "globals": {
    "window": false,
    "global": false,
    "document": false,
    "describe": false,
    "shallow": false,
    "it": false,
    "expect": false,
    "fetch": false,
    "escapeSnapshot": false,
    "jest": false,
    "mount": false
  }
}
